@article{bravo-oviedo_dominant_2008,
	title = {Dominant height growth equations including site attributes in the generalized algebraic difference approach},
	volume = {38},
	issn = {0045-5067},
	url = {https://cdnsciencepub.com/doi/10.1139/X08-077},
	doi = {10.1139/X08-077},
	abstract = {We present a new dynamic dominant height growth model based on Cieszewski’s generalized algebraic difference approach (GADA) advanced dynamic site equation strengthened by the use of explicit climate and soil variables (i.e., H = f(H0,T0, T, site conditions)). The results suggest that the inclusion of climatic variables would improve the applicability of the inter-regional model in regions in which climate and soil type lead to intra-regional variability. The new model reduces the bias present in a previous dynamic model that did not include climatic attributes and improves the model efficiency across the different age classes. Climate has a multiplicative effect on dominant tree growth in the early development stages ({\textless}20 years) and an additive effect in older stands.},
